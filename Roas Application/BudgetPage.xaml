<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Roas_Application.BudgetPage">
    <ContentPage.Content>
        <StackLayout>
            <Label  Text="Budget"></Label>
            <Entry x:Name="budget" Placeholder="Budget" Keyboard="Numeric"></Entry>
            <Button Text="Next" Clicked="OnNextButton_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>